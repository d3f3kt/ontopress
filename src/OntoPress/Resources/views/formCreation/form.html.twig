{%- raw -%}
<form name='{{ form.vars.name }}' method='{{ form.vars.method }}' action='{{ form.vars.action }}' enctype='multipart/form-data'>
{%- endraw %}
<table class='form-table' id='OntoPressForm'>
    {% for formField in formFields -%}
    <tr>
        {%- include 'formCreation/formField.html.twig' %}
    </tr>
    {% endfor -%}
    <tr>
        <td>
            <button class="button button-primary" name="OntoPressForm[submit]" id="OntoPressForm_submit" type="submit">Weiter</button>
        </td>
    </tr>
</table>
</form>
